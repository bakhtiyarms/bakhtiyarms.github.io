{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "data": {
    "url": "https://raw.githubusercontent.com/bakhtiyarms/bakhtiyarms.github.io/refs/heads/main/fixed.csv",
    "format": {
      "type": "dsv",
      "delimiter": ";"
    }
  },
  "mark": "trail",
  "width": 500,
  "height": 350,
  "title": {
    "text": "UK Public Sector Net Debt since 1700"
  },
  "encoding": {
    "x": {
      "field": "year",
      "type": "temporal"  
    },
    "y": {
      "field": "value",
      "type": "quantitative"
    },
    "color": {
      "field": "series"
    }
  }
}